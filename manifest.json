{
	"content_scripts": [
		{
			"js": ["addDelicious.js","getSelection.js"],
			"css": ["style.css"],
			"matches": ["http://*/*","https://*/*"]
		}
	],
	"name": "Del.icio.us Tools",
	"version": "1.0.2",
	"description": "Del.icio.us toolbar for Chrome",
	"permissions": [
		"http://delicious.com/*",
		"http://feeds.delicious.com/*",
		"tabs",
		"http://*/",
		"https://*/"
	],
	"browser_action": {
		"default_title": "Delicious",
		"default_icon": "delicious.png",
		"popup": "popup.html"
	},
	"background_page": "background.html",
	"update_url": "http://delicious-chrome-extension.googlecode.com/files/updates.xml"
}

